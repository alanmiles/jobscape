<%= link_to image_tag("pageback.png", :title => "Previous"), corporate_plan_path(@business), :class => "magnify" %>

<h4 class="over"><%= @business.name_city %></h4>

<%= form_for @objective do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  
  <div class="shift_center_50">
  
    <%= render 'fields', :f => f %>
  
    <div class="actions">
      <%= f.submit :class => "create round" %>
    </div>
  </div>
<% end %>
