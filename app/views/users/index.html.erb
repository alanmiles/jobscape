<h1>All users</h1>

<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li><%= user.id %>
        | <%= link_to user.name, user %> 
        | <%= user.city %>
  	| <%= user.account_type %>
  	| <%= link_to "delete", user, :method => :delete, :confirm => "You sure?",
                                :title => "Delete #{user.name}" %>

    </li>
  <% end %>
</ul>

<%= will_paginate %>
