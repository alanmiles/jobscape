<div class="r-float">
  <%= link_to "Entire portrait", user_portrait_path(@previousjob.user) %>
</div>

<%= link_to image_tag("pageback.png", :title => "Previous jobs list"), user_previousjobs_path(@previousjob.user_id), :class => "magnify" %>

<h4 class="over"><%= @previousjob.user.name %></h4>

<div class="shift_center_50">
  <%= form_for @previousjob do |f| %>
    <%= render 'fields', :f => f %> 
    <div class="actions">
      <%= f.submit "Update the Job", :class => "create round" %>
    </div>
  <% end %>
</div>
