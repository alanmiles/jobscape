<div class="r-float">
  <%= link_to "Entire portrait", user_portrait_path(@qualification.user) %>
</div>

<%= link_to image_tag("pageback.png", :title => "Qualifications list"), user_qualifications_path(@qualification.user), :class => "magnify" %>

<h4 class="over"><%= @qualification.user.name %></h4>

<div class="shift_center_50">
  <%= form_for @qualification do |f| %>
    <%= render 'fields', :f => f %> 
    <div class="actions">
      <%= f.submit :class => "create round" %>
    </div>
  <% end %>
</div>
