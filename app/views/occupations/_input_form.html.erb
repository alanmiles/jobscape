<%= link_to image_tag("pageback.png", :title => "Previous"), occupations_path, :class => "magnify" %>

<div class ="shift_center_50">
  <%= form_for(@occupation) do |f| %>
    <%= render 'shared/error_messages', :object => f.object %>
    <div class="field">
      <%= f.label :name, "Occupation name *" %><br />
      <%= f.text_field :name %>
    </div>
    <div class="actions">
      <%= f.submit :class => "create round" %>
    </div>
  <% end %>
</div>
