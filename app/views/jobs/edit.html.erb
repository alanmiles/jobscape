<%= link_to image_tag("pageback.png", :title => "Previous"), business_jobs_path(@business), :class => "magnify" %>

<h4 class="over"><%= @business.name_city unless @user.account == 1 %></h4>

<%= form_for(@job) do |f| %>
  <div class="shift_center_50">
    <%= render 'fields', :f => f %>
    <div class="actions">
      <%= f.submit :class => "create round" %>
    </div>
  </div>
<% end %>
